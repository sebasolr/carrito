<table class="table align-middle mb-0 bg-white container-sm">
  <thead class="bg-light">
    <tr>
      <th>id</th>
      <th>Fecha de compra</th>
      <th>Producto</th>
      <th>Codigo</th>
      <th>Precio</th>
      <th>Stock</th>
      <th>img</th>
    </tr>
  </thead>
  <tbody>
    {{#each articulos}}
    <tr>
      <td>
        <p class="fw-normal mb-1">{{this.id}}</p>
      </td>
      <td>
        <p class="fw-bold mb-1">{{this.fecha}}</p>
      </td>
      <td>
        <p class="fw-bold mb-1">{{this.titulo}}</p>
      </td>
      <td>
        <p class="fw-normal mb-1">{{this.codigo}}</p>
      </td>
      <td>
        <p class="fw-normal mb-1">{{this.precio}}</p>
      </td>
      <td>
        <p class="fw-normal mb-1">{{this.stock}}</p>
      </td>
      <td>
        <div class="d-flex align-items-center">
          <img
              src="{{this.url}}"
              alt=""
              style="width: 45px; height: 45px"
              class="rounded-circle"
              />
          <div class="ms-3">            
          </div>
        </div>
      </td>
    </tr>
    {{/each}}
        <tr>
      <th>  </th>
      <th>         </th>
      <th>       </th>
      <th>       </th>
      <th>      </th>
      <th>TOTAL</th>
      <th>{{{subtotal}}}     </th>
    </tr>
  </thead>
  </tbody>
</table>
